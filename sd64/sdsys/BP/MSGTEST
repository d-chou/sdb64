PROGRAM MSGTEST
* 03/31/22 - NextJob test program for messages error
PRINT 'HERE WE GO'
*
OPENPATH "/usr/local/sdsys/MESSAGES" TO MSGFV ELSE ABORT "Cannot open MESSAGES"
*
SSELECT MSGFV TO 2 
*
READLIST MSGLIST FROM 2 THEN
*
  MSGCNT = DCOUNT(MSGLIST,@AM)
*
  PRINT 'MESSAGE COUNT: ':MSGCNT
*
  FOR I = 1 TO 1030
    IF NUM(MSGLIST<I>) THEN
      PRINT "1: ":I: ' ':MSGLIST<I>:' ':SYSMSG(MSGLIST<I>)
    END 
  NEXT I
*
END
*
PRINT 'COMPLETE'
END
